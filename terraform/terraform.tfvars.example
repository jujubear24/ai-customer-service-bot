<!-- The AWS region to deploy the backend resources  -->

aws_region = "us-east-1"

<!-- The unique name for this project (from your setup script). -->
project_name = "ai-customer-service-bot"

<!-- Your 12-digit AWS Account ID (without hyphens). -->
aws_account_id = "123456789012"

<!-- Common tags -->
common_tags = {
  Project     = "ai-customer-service-bot"
  Environment = "backend-bootstrap"
  ManagedBy   = "terraform"
}
